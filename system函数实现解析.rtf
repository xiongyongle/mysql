int mysystem(const char* command)\par
\{\par
    pid_t pid;\par
    int status;\par
    struct sigaction ignore,saveintr,savequit;\par
    sigset_t childmask,savemask;\par
    if(str == NULL)\par
    \{\par
        return 1;\par
    \}\par
    //\'ca\'d7\'cf\'c8\'b2\'bb\'c4\'dc\'c8\'c3\'d6\'f7\'bd\'f8\'b3\'cc\'bd\'d3\'ca\'d5\'b5\'bdSIGINT,SIGQUIT\'d0\'c5\'ba\'c5\par
    ignore.sa_handler = SIG_IGN;\par
    sigemptyset(&ignore.sa_mask);\par
    ignore.sa_flags = 0;\par
    if(sigaction(SIGINT,&ignore,&saveintr)<0)\par
    \{\par
        return -1;\par
    \}\par
    if(sigaction(SIGQUIT,&ignore,&savequit)<0)\par
    \{\par
        return -1;\par
    \}\par
    sigemptyset(&childmask);\par
    sigaddset(&childmask,SIGCHLD);\par
    if(sigprocmask(SIG_BLOCK,&childmask,&savemask)<0)\par
    \{\par
        return -1;\par
    \}\par
    print_signal("begin:");\par
    pid = fork();\par
    if(pid < 0)\par
    \{\par
        return -1;\par
    \}\par
    if(pid == 0)\par
    \{//\'d4\'da\'d7\'d3\'bd\'f8\'b3\'cc\'d6\'d0\'bb\'d6\'b8\'b4SIGINT,SIGQUIT\'b5\'c4\'cf\'c8\'c7\'b0\'d7\'b4\'cc\'ac\par
        sigaction(SIGINT,&saveintr,NULL);\par
        sigaction(SIGQUIT,&savequit,NULL);\par
        sigprocmask(SIG_SETMASK,&savemask,NULL)\'a3\'bb\par
        execl("/bin/sh","sh","-c",command,NULL);\par
        _exit(127); //\'ca\'c7\'ce\'aa\'c1\'cb\'b7\'c0\'d6\'b9execl\'b3\'f6\'b4\'ed\'b5\'c4\'ca\'b1\'ba\'f2\'d3\'c3\'b5\'c4,\'c8\'e7\'b9\'fbexecl\'b3\'c9\'b9\'a6\'d4\'f2\'b2\'bb\'bb\'e1\'d6\'b4\'d0\'d0\'d5\'e2\'cc\'f5\'d3\'ef\'be\'e4\par
    \}\par
    else\par
    \{\par
        while(waitpid(pid,&status,0)<0)  //waitpid\'d3\'ebSIGCHLD\'c3\'bb\'d3\'d0\'b1\'d8\'c8\'bb\'b5\'c4\'b9\'d8\'cf\'b5\par
        \{\par
            if(errno != EINTR)\par
            \{\par
                status = -1;\par
                break;\par
            \}\par
        \}\par
        \}\par
    \}\par
    //\'bb\'d6\'b8\'b4\'b8\'f7\'d6\'d6\'d0\'c5\'ba\'c5\'b5\'bd\'b5\'f7\'d3\'c3system\'d6\'ae\'c7\'b0\'b5\'c4\'d7\'b4\'cc\'ac\par
    if(sigaction(SIGINT,&saveintr,NULL)<0)\par
    \{\par
        return -1;\par
    \}\par
    if(sigaction(SIGQUIT,&savequit,NULL)<0)\par
    \{\par
        return -1;\par
    \}\par
    if(sigprocmask(SIG_SETMASK,&savemask,NULL)<0)\par
    \{\par
        return -1;\par
    \}//\'d4\'da\'d5\'e2\'c0\'ef\'d6\'b4\'d0\'d0\'cd\'ea\'d6\'ae\'ba\'f3,\'b2\'c5\'bb\'e1\'d6\'b4\'d0\'d0\'d0\'c5\'ba\'c5\'b4\'a6\'c0\'ed\'ba\'af\'ca\'fd(\'c8\'e7\'b9\'fb\'d3\'d0\'b5\'c4\'bb\'b0)\par
    return status;\par
\}\par
